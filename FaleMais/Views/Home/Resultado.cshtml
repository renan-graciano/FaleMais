@model List<Dominio.PlanoComparacao>
<div class="col-md-12">
    <div class="panel panel-primary list-panel" id="list-panel" style="width:100%">
        <div class="panel-heading list-panel-heading">
            <h1 class="panel-title list-panel-title">Valores por minuto</h1>
        </div>
        <div class="panel-footer">
            <table id="planComp-data-table"
                   class="table table-striped table-bordered"
                   style="width:100%">
                <thead>
                    <tr>
                        <th>Origem</th>
                        <th>Destino</th>
                        <th>Tempo</th>
                        <th>Plano</th>
                        <th>ComPlano</th>
                        <th>SemPlano</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var valores in Model)
                    {
                        <tr>
                            <td>@valores.Origem</td>
                            <td>@valores.Destino</td>
                            <td>@valores.Tempo</td>
                            <td>@valores.Plano</td>
                            <td>@valores.ComPlano.ToString("0.00")</td>
                            <td>@valores.SemPlano.ToString("0.00")</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
